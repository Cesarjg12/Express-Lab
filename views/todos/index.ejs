<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Daily Mornings</h1>
    <ul>
      <% todos.forEach(function(todo) { %>
        <li>
          <%= todo.task %>
            - 
          <%= todo.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>